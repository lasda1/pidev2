{% extends 'baseBack.html.twig' %}

{% block body %}
    <div class="col-sm-12">
        <div class="widget widget-fullwidth widget-small">
            <div class="widget-head">

                <div class="title">Objets</div>
            </div>
            <div class="table-responsive noSwipe">
                <table class="table table-striped table-fw-widget table-hover">
                    <thead>
                    <tr>
                        <th width="20%">User</th>
                        <th width="20%">Photo</th>
                        <th width="25%">Date</th>
                        <th width="10%">Type</th>
                        <th width="10%">Description</th>
                        <th width="10%">Lieu</th>
                        <th width="10%">Nature</th>
                    </tr>
                    </thead>
                    {% for objet in objet %}
                        <tbody class="no-border-x">
                        <tr>
                            <td>{{ objet.User }}</td>
                            <td class="user-avatar"> <img src="{{ asset(objet.Photo) }}"></td>
                            <td>{{ objet.Date |date }}</td>
                            <td>{{ objet.Type }}</td>
                            <td>{{ objet.Description }}</td>
                            <td>{{ objet.Lieu }}</td>
                            <td>{{ objet.Nature }}</td>
                        </tr>
                        </tbody>
                    {% endfor %}
                </table>
            </div>
        </div>
    </div>
{% endblock %}